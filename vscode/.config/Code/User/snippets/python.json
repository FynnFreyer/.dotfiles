{
    "Main Guard": {
        "prefix": "main",
        "body": [
            "if __name__ == \"__main__\":",
            "\t${0:main()}"
        ],
        "description": "Standard execution guard"
    },
    "Typed Function": {
        "prefix": "def",
        "body": [
            "def ${1:func_name}(${2:param}: ${3:type}) -> ${4:None}:",
            "\t\"\"\"",
            "\t${5:Short description...}",
            "",
            "\t:param ${2:param}: ${6:description}",
            "\t:return: ${7:description}",
            "\t\"\"\"",
            "\t${0:pass}"
        ],
        "description": "Creates a function with type hints and reST docstring"
    },
}
